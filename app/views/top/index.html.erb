<div class="wrapper wrapper-body">
<div class="inner inner-body">
    <div class="row">
        <div class="col-md-4">
            <div class="matching">
                <div id="matching-container">
                </div>
            </div>
        </div>
        <div class="col-md-8">
          <div class="row">
            <div class="row selector col-md-6">
                <h3>アウター</h3>
                <div class="col-md-6 selector-item">
                  <div class="col-md-6 selector-item selector-item-child">
                    <%= image_tag '/assets/outer1.jpg', class: 'select-img' %>
                  </div>
                  <div class="col-md-6 selector-item selector-item-button">
                    <div class="btn btn-default">試着</div>
                    <div class="btn btn-warning">カートへ追加</div>
                  </div>
                </div>
                <div class="col-md-6 selector-item">
                  <div class="col-md-6 selector-item selector-item-child">
                    <%= image_tag '/assets/outer2.jpg', class: 'select-img' %>
                  </div>
                  <div class="col-md-6 selector-item selector-item-button">
                    <div class="btn btn-default">試着</div>
                    <div class="btn btn-warning">カートへ追加</div>
                  </div>
                </div>
            </div>
            <div class="row selector col-md-6">
                <h3>インナー</h3>
                <div class="col-md-6 selector-item">
                  <div class="col-md-6 selector-item selector-item-child">
                    <%= image_tag '/assets/inner1.jpg', class: 'select-img' %>
                  </div>
                  <div class="col-md-6 selector-item selector-item-button">
                    <div class="btn btn-default">試着</div>
                    <div class="btn btn-warning">カートへ追加</div>
                  </div>
                </div>
                <div class="col-md-6 selector-item">
                  <div class="col-md-6 selector-item selector-item-child">
                    <%= image_tag '/assets/inner2.jpg', class: 'select-img' %>
                  </div>
                  <div class="col-md-6 selector-item selector-item-button">
                    <div class="btn btn-default">試着</div>
                    <div class="btn btn-warning">カートへ追加</div>
                  </div>
                </div>
            </div>
          </div>
          <div class="row">
            <div class="row selector col-md-6">
                <h3>ボトムス</h3>
                <div class="col-md-6 selector-item">
                  <div class="col-md-6 selector-item selector-item-child">
                    <%= image_tag '/assets/bottoms.jpg', class: 'select-img' %>
                  </div>
                  <div class="col-md-6 selector-item selector-item-button">
                    <div class="btn btn-default">試着</div>
                    <div class="btn btn-warning">カートへ追加</div>
                  </div>
                </div>
                <div class="col-md-6 selector-item">
                  <div class="col-md-6 selector-item selector-item-child">
                    <%= image_tag '/assets/souldout.jpg', class: 'select-img' %>
                  </div>
                  <div class="col-md-6 selector-item selector-item-button">
                  </div>
                </div>
            </div>
          </div>
          <div class="row">
            <div class="row selector col-md-6">
                <h3 id="testtest">ボトムス</h3>
            </div>
          </div>
        </div>
    </div>
</div>
</div>
